<Window x:Class="Viewer.Windows.PatternWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:controls="clr-namespace:Viewer.Controls"
        mc:Ignorable="d"
        Title="PatternWindow" Height="640" Width="720">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="*"></RowDefinition>
        </Grid.RowDefinitions>
        <controls:CandlesViewer Margin="6" Grid.Row="0" x:Name="Viewer"></controls:CandlesViewer>
    </Grid>
</Window>
