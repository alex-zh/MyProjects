             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             mc:Ignorable="d" d:DesignWidth="62" d:DesignHeight="107" 
        >
    <Grid x:Name="grid" Height="45" Background="Transparent" >
        <Border BorderThickness="0,1,1,0 " BorderBrush="LightGray" >
            <Canvas x:Name="canvasAxis" Background="Transparent"></Canvas>
        </Border>
    </Grid>
</UserControl>
